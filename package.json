{
  "name": "naturos",
  "version": "1.0.0",
  "description": "Landing page for Naturos",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss styles/style.css -w",
    "compile:sass": "node-sass sass/main.scss styles/style.comp.css",
    "concat:css": "concat -o styles/style.concat.css styles/icon-fonts.css styles/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 verisons' styles/style.concat.css -o styles/style.prefix.css",
    "compress:css": "node-sass styles/style.prefix.css styles/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Khudoyberdi Shamsiyev",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concat": "^1.0.3",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  }
}
